{
  "manifest_version": 2,
  "name": "Music",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js","soundmanager2.js", "audiomanager.js", "soundcloud.js", "akshay_content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["jquery.min.js","new_background.js"]
    },
  "permissions": [
        "http://play-hackmit.rhcloud.com/*",
        "http://localhost/*"
    ]
}

